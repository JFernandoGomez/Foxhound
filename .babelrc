{
	"presets": [
		[ "env", {
			"targets": {
				"browsers": [
					"last 2 versions",
					"Safari >= 10",
					"iOS >= 10",
					"not ie <= 10"
				]
			}
		} ],
		"stage-2"
	],
	"plugins": [
		"add-module-exports",
		"syntax-jsx",
		"transform-export-extensions",
		"transform-react-jsx",
		"transform-runtime",
	],
	"env": {
		"test": {
			"plugins": [
				[ "babel-plugin-webpack-alias", { "config": "./webpack.config.js" } ]
			]
		}
	}
}
